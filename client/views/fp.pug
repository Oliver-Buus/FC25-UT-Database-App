doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body
        table
            thead 
                tr 
                    th SPILLER 
                    th OVERALL
                    th CLUB
                    th LEAGUE
            tbody 
                each player in players 
                    tr 
                        td 
                            a(href=player.url, target="_blank")= player.name
                        td= player.overall
                        td= player.club
                        td= player.league
                        